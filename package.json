    "@vercel/speed-insights": "^1.2.0",
    "framer-motion": "^12.23.24",
    "googleapis": "^166.0.0",
    "next": "16.0.5",
    "node-ical": "^0.22.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "redis": "^5.10.0",
    "remark-gfm": "^4.0.1",
